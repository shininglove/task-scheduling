{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "noEmit": true,
    "noUncheckedSideEffectImports": true,
    "path": {
      "$lib/*" : ["src/lib/*.ts"]
    },
  },
  "include": ["vite.config.ts"]
}
